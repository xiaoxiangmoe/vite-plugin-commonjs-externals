{
  "name": "vite-plugin-commonjs-externals",
  "version": "0.1.2",
  "repository": "https://github.com/xiaoxiangmoe/vite-plugin-commonjs-externals.git",
  "author": "ZHAO Jinxiang <xiaoxiangmoe@gmail.com>",
  "license": "MIT",
  "keywords": [
    "vite-plugin",
    "commonjs"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "default": "./dist/index.cjs"
  },
  "scripts": {
    "build": "rollup -c ./rollup.config.mjs && mv dist/index-cjs.d.ts dist/index.d.cts",
    "prepack": "yarn run build"
  },
  "dependencies": {
    "acorn": "^8.8.2",
    "es-module-lexer": "^1.2.1",
    "magic-string": "^0.30.0"
  },
  "devDependencies": {
    "@types/estree": "^1.0.1",
    "prettier": "^2.8.8",
    "rollup": "^3.22.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.7"
  }
}
