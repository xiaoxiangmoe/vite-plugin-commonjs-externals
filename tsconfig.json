{
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "target": "ES2017",
    "module": "ESNext",
    "moduleDetection": "force",
    "moduleResolution": "bundler",
    "declaration": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true
  }
}
